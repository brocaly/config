# General
super + Return
	kitty
super + n
	simplenote
super + c
	/usr/bin/pulseaudio-ctl down ; \
	~/.config/scripts/sbu.sh
super + v
	/usr/bin/pulseaudio-ctl up ; \
	~/.config/scripts/sbu.sh
super + q
	kitty -e qalc
super + r
	kitty -e ranger
super + w
	brave
super + <
	mpc next
super + >
	mpc prev
super + shift + n
	kitty -e ncmpcpp
super + shift + b
	nautilus
super + shift + f
	freecad
super + shift + h
	kitty -e htop
super + shift + l
	i3lock -c 333333
super + shift + o
	libreoffice
super + shift + r
	killall sxhkd & ; \
	sxhkd </dev/null &>/dev/null &
super + shift + s
	scrot -p
super + alt + c
	code
super + alt + p
	i3lock -c 333333 & ; \
	systemctl suspend

# mpc
super + shift + p
	mpc toggle

# Games
super + d
	discord
super + s
	steam -console
super + shift + m
	minecraft-launcher
