# General
super + Return
	kitty
super + n
	simplenote
super + c
	/usr/bin/pulseaudio-ctl down ; \
	~/.config/scripts/sbu.sh
super + v
	/usr/bin/pulseaudio-ctl up ; \
	~/.config/scripts/sbu.sh
super + q
	kitty -e qalc
super + r
	kitty -e ranger
super + w
	brave
super + <
	mpc next
super + >
	mpc prev
super + shift + l
	i3lock -efk -i ~/Pictures/wallpapers/wp2.jpg
super + shift + n
	kitty -e ncmpcpp
super + shift + s
	brave </dev/null &>/dev/null & ; \
	discord </dev/null &>/dev/null & ; \
	steam </dev/null &>/dev/null &
super + shift + r + h
	killall sxhkd & ; \
	sxhkd </dev/null &>/dev/null &
super + alt + p
	systemctl suspend

# mpc
super + shift + p
	mpc toggle

# Games
super + d
	discord
super + s
	steam
super + shift + m
	minecraft-launcher

